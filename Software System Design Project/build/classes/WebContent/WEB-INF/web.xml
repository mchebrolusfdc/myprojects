</servlet>
<servlet-mapping>
<servlet-name>DoctorManageTimings</servlet-name>
<url-pattern>/DoctorManageTimings</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>GetDoctorDetailsServlet</servlet-name>
