</servlet>
<servlet-mapping>
<servlet-name>GetDoctorDetailsServlet</servlet-name>
<url-pattern>/GetDoctorDetailsServlet</url-pattern>
</servlet-mapping>
<servlet>
<servlet-name>AdminSlotServlet</servlet-name>
