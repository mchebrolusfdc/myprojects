<!DOCTYPE html>
<html lang ="en">
    <head>
        <title>connections</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
         <link rel="stylesheet" type="text/css" href="../assets/stylesheets/indexstyles.css">
    </head>
    <body>
	 <% include partials/header %>

        <main>
            <div class="homeDescriptionContainer">
              <% Object.keys(data).forEach(function(index) { %>
            <div class=<%= index %> >
              <img src=<%= "../assets/images/"+index+".png" +' alt="image4"' %> />
              <div class=<%=index+"content"%> >
                <div id=<%= "category"+index %> >
                  <h3> <%= index %> </h3>
                </div>
              <div>
                <ul class=<%=index+"list"%> >
                  <% data[index].forEach(function(val) {%>
             <li> <a href="/connections?connectionID=<%= val[1] %>"> <%= val[0] %> </a> </li>
           <% }); %>
         </ul>
       </div>
   </div>
   </div>
     <% }); %>
            </div>
        </main>
        <% include partials/footer.ejs%>
    </body>
</html>
